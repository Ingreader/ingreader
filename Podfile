source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '8.0'
project 'ingreader'

inhibit_all_warnings!

target 'ingreader' do
	pod 'TesseractOCRiOS', :git => 'https://github.com/gali8/Tesseract-OCR-iOS.git'
	pod 'ImageMagick', '= 6.8.8-9'
	pod 'Reveal-iOS-SDK', :configurations => ['Debug']
end